{
  "name": "dipper.js",
  "version": "0.2.2",
  "description": "A state machine library for TypeScript",
  "main": "lib/index.js",
  "types": "lib/index/d/ts",
  "scripts": {
    "start": "npm run build:live",
    "build": "rm -rf lib/* ; tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' examples/run-examples.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "publisher": "npm run build ; npm publish",
    "republish": "npm unpublish --force ; npm run publisher"
  },
  "keywords": [
    "state",
    "states",
    "machine",
    "state machine",
    "finite state machine",
    "event-driven finite state machine",
    "state management"
  ],
  "repository": {
    "url": "https://github.com/gbrunow/dipper",
    "type": "git"
  },
  "author": "Guilherme Brunow Gomes",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^10.12.19",
    "node-emoji": "^1.8.1",
    "nodemon": "^1.18.9",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.4"
  },
  "dependencies": {
    "rxjs": "^6.3.3"
  }
}